{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
		"linebreak-style": ["error", "windows"],
		"jsx-quotes": ["error", "prefer-single"],
        "jsx-a11y/label-has-associated-control": "off"
    }
}
